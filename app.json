{
  "expo": {
    "name": "Mini_Cart",
    "slug": "Mini_Cart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/miniIcon.png",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/picture.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/picture.png"
    }
  }
}